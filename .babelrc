{
    "presets": [
        [
            "env",
            {
                "modules": false,
                "targets": {
                    "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
                }
            }
        ],
        "react",
        "stage-2"
    ],
    "plugins": [
        ["import", { "libraryName": "antd", "style": true }],
        "transform-runtime"
    ],
    "comments": false
}
